datasource postgres {
  provider = "postgresql"
  url      = env("POSTGRESQL_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt DateTime @default(now())
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     String[]
  updatedAt DateTime @updatedAt
  username  String   @unique
}

model Test {
  age         Int?
  createdAt   DateTime @default(now())
  dateOfBirth Json?
  id          String   @id @default(cuid())
  name        String?
  updatedAt   DateTime @updatedAt
}